<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="Control.Focus" 
                     Color="#FFAAFF00" />
    
    <SolidColorBrush x:Key="Window.Background" 
                     Color="#FF444444" />

    <SolidColorBrush x:Key="TextBlock.Foreground" 
                     Color="#FFFFFFFF" />

    <SolidColorBrush x:Key="Button.Defaulted.Background" 
                     Color="#FF444444" />
    <SolidColorBrush x:Key="Button.Defaulted.Foreground" 
                     Color="#FFFFFFFF" />
    <SolidColorBrush x:Key="Button.Defaulted.Border" 
                     Color="#FF525252" />

    <SolidColorBrush x:Key="Button.Static.Background" 
                     Color="#FF525252" />
    <SolidColorBrush x:Key="Button.Static.Foreground" 
                     Color="#FFFFFFFF" />
    <SolidColorBrush x:Key="Button.Static.Border" 
                     Color="#FF525252" />

    <SolidColorBrush x:Key="Button.Pressed.Background" 
                     Color="#FFAAFF00" />
    <SolidColorBrush x:Key="Button.Pressed.Foreground" 
                     Color="#FFFFFFFF" />
    <SolidColorBrush x:Key="Button.Pressed.Border" 
                     Color="#FFAAFF00" />

    <SolidColorBrush x:Key="Button.Disabled.Background" 
                     Color="#FF525252" />
    <SolidColorBrush x:Key="Button.Disabled.Foreground" 
                     Color="#FF666666" />
    <SolidColorBrush x:Key="Button.Disabled.Border" 
                     Color="#FF525252" />

    <SolidColorBrush x:Key="ScrollBar.Static.Thumb" 
                     Color="#FF595959" />
    <SolidColorBrush x:Key="ScrollBar.MouseOver.Thumb" 
                     Color="#FF595959" />
    <SolidColorBrush x:Key="ScrollBar.Pressed.Thumb" 
                     Color="#FFAAFF00" />

    <SolidColorBrush x:Key="TextBox.Static.Background" 
                     Color="#FFFFFFFF" />
    <SolidColorBrush x:Key="TextBox.Static.Foreground" 
                     Color="#FF000000" />
    <SolidColorBrush x:Key="TextBox.Static.Border" 
                     Opacity="0" />
    
    <SolidColorBrush x:Key="TextBox.Focus.Background" 
                     Color="#FFFFFFFF" />
    <SolidColorBrush x:Key="TextBox.Focus.Foreground" 
                     Color="#FF000000" />
    <SolidColorBrush x:Key="TextBox.Focus.Border" 
                     Color="#FFAAFF00" 
                     Opacity="1" />
    
    <SolidColorBrush x:Key="TextBox.Disabled.Background" 
                     Color="#FFFFFFFF" />
    <SolidColorBrush x:Key="TextBox.Disabled.Foreground" 
                     Color="#FF000000" />
    <SolidColorBrush x:Key="TextBox.Disabled.Border" 
                     Opacity="0" />

</ResourceDictionary>