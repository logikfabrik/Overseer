<UserControl
    x:Class="Logikfabrik.Overseer.WPF.Controls.Icon"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    x:Name="UserControl"
    d:DesignHeight="300"
    d:DesignWidth="300"
    mc:Ignorable="d">
    <Viewbox Stretch="Uniform">
        <Path
            Data="{Binding Data, Mode=OneWay, ElementName=UserControl}"
            Fill="{Binding Foreground, Mode=OneWay, ElementName=UserControl}"
            Stretch="Uniform" />
    </Viewbox>
</UserControl>
